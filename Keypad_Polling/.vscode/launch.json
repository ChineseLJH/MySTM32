{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cortex Debug STLink",
            "cwd": "${workspaceFolder}",
            "executable": "./build/Debug/Keypad_Polling.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "openocd",
            "device": "STM32F103C8",
            "configFiles": [
                "interface/stlink-v2.cfg",
                "target/stm32f1x.cfg"
            ],
            "gdbPath": "/usr/bin/gdb-multiarch"
        }
    ]
}