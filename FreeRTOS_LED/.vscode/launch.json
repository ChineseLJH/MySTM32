    "version": "0.2.0",
    "configurations": [

        {
            "name": "Cortex Debug(ST-Link)",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/Debug/FreeRTOS_LED.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "openocd",
